
class Result
{

    /*
     * Complete the 'staircase' function below.
     *
     * The function accepts INTEGER n as parameter.
     */

    public static void staircase(int n)
    {
        for (int i=1; i<=n; i++) {
            int space = n-i;
            for (int j=0; j<space; j++) {
                Console.Write(' ');
            }
            for (int j=i; j>0; j--) {
                Console.Write('#');
            }
            Console.Write('\n');
        } 
    }
}